// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+w ctrl+b",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+y",
        "command": "workbench.action.toggleActivityBarVisibility",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+e",
        "command": "workbench.view.explorer",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+f",
        "command": "workbench.view.search",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+v",
        "command": "workbench.view.scm",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+d",
        "command": "workbench.view.debug",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+x",
        "command": "workbench.view.extensions",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w =",
        "command": "workbench.action.evenEditorWidths",
        "when": "!editorTextFocus"
    },
    {
        "command": "workbench.action.focusActiveEditorGroup",
        "key": "ctrl+w ctrl+w",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w ctrl+k",
        "command": "workbench.action.keepEditor",
        "when": "!editorTextFocus"
    },
    {
        "key": "ctrl+w e",
        "command": "workbench.action.quickOpen",
        "when": "!editorFocus"
    },
    {
        "key": "ctrl+y",
        "command": "list.select",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+n",
        "command": "editor.action.triggerSuggest",
        "when": "vim.mode == 'Insert' && editorHasCompletionItemProvider && editorTextFocus && !editorReadonly && !suggestWidgetVisible"
    },
    {
        "key": "ctrl+p",
        "command": "editor.action.triggerSuggest",
        "when": "vim.mode == 'Insert' && editorHasCompletionItemProvider && editorTextFocus && !editorReadonly && !suggestWidgetVisible"
    },
    {
        "key": "ctrl+y",
        "command": "acceptSelectedSuggestionOnEnter",
        "when": "acceptSuggestionOnEnter && editorTextFocus && suggestWidgetVisible && suggestionMakesTextEdit"
    },
    {
        "key": "ctrl+e",
        "command": "hideSuggestWidget",
        "when": "editorTextFocus && suggestWidgetVisible"
    },
    {
        "key": "ctrl+p",
        "command": "workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "workbench.action.focusQuickOpen",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "command": "workbench.action.closeQuickOpen",
        "key": "ctrl+[",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "meta+j",
        "command": "workbench.action.openNextRecentlyUsedEditorInGroup"
    },
    {
        "key": "meta+j",
        "command": "workbench.action.quickOpenNavigateNextInEditorPicker",
        "when": "inEditorsPicker && inQuickOpen"
    },
    {
        "key": "meta+k",
        "command": "workbench.action.openPreviousRecentlyUsedEditorInGroup"
    },
    {
        "key": "meta+k",
        "command": "workbench.action.quickOpenNavigatePreviousInEditorPicker",
        "when": "inEditorsPicker && inQuickOpen"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+r",
        "command": "-workbench.action.quickOpenNavigateNextInRecentFilesPicker",
        "when": "inQuickOpen && inRecentFilesPicker"
    },
    {
        "key": "ctrl+shift+r",
        "command": "-workbench.action.quickOpenNavigatePreviousInRecentFilesPicker",
        "when": "inQuickOpen && inRecentFilesPicker"
    },
    {
        "key": "shift+escape",
        "command": "-workbench.action.closeQuickOpen",
        "when": "inQuickOpen"
    },
    {
        "key": "ctrl+space",
        "command": "-editor.action.triggerSuggest",
        "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+h",
        "command": "-editor.action.startFindReplaceAction"
    },
    {
        "key": "ctrl+shift+g",
        "command": "-workbench.view.scm"
    },
    {
        "key": "ctrl+g",
        "command": "-workbench.action.gotoLine"
    },
    {
        "key": "ctrl+-",
        "command": "-workbench.action.zoomOut"
    },
    {
        "key": "ctrl+=",
        "command": "-workbench.action.zoomIn"
    },
    {
        "key": "ctrl+shift+=",
        "command": "-workbench.action.zoomIn"
    },
    {
        "key": "ctrl+m",
        "command": "-editor.action.toggleTabFocusMode"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeWindow",
        "when": "!editorIsOpen"
    },
    {
        "key": "ctrl+w",
        "command": "-workbench.action.closeActiveEditor"
    },
    {
        "key": "ctrl+p",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+shift+e",
        "command": "-workbench.view.explorer"
    },
    {
        "key": "ctrl+n",
        "command": "-workbench.action.files.newUntitledFile"
    },
    {
        "key": "ctrl+shift+n",
        "command": "-workbench.action.newWindow"
    },
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpenNavigateNextInFilePicker",
        "when": "inFilesPicker && inQuickOpen"
    },
    {
        "key": "ctrl+j",
        "command": "-workbench.action.togglePanel"
    },
    {
        "key": "ctrl+k enter",
        "command": "-workbench.action.keepEditor"
    },
    {
        "key": "ctrl+/",
        "command": "-editor.action.commentLine",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+k ctrl+/",
        "command": "-editor.foldAllBlockComments",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+]",
        "command": "-editor.action.indentLines",
        "when": "editorTextFocus && !editorReadonly"
    }
]
