{
  "breadcrumbs.enabled": false,
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": false,
  "editor.cursorStyle": "block",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.find.seedSearchStringFromSelection": false,
  "editor.fontFamily": "'Inconsolata LGC'",
  "editor.fontSize": 17,
  "editor.formatOnSave": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "editor.links": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.renderIndentGuides": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "selection",
  "editor.rulers": [72],
  "editor.suggestOnTriggerCharacters": false,
  "explorer.enableDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "force-expand-multiline",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "update.mode": "none",
  "window.menuBarVisibility": "hidden",
  "window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "workbench.activityBar.visible": false,
  "workbench.colorCustomizations": {
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
},
  "workbench.editor.showTabs": false,
  "workbench.editor.limit.value": 4,
  "workbench.editor.limit.enabled": true,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zenMode.hideStatusBar": false,
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#005f5f",
  "vim.statusBarColors.insert": "#c0392b",
  "vim.statusBarColors.visual": "#913D88",
  "vim.statusBarColors.visualline": "#593281",
  "vim.statusBarColors.visualblock": "#86592d",
  "vim.statusBarColors.replace": "#000000",
  "vim.foldfix": true,
  "explorer.confirmDelete": false,
  "editor.semanticHighlighting.enabled": false,
  "window.restoreWindows": "one",
  "diffEditor.ignoreTrimWhitespace": false
}
