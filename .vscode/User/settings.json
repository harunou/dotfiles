{
  "breadcrumbs.enabled": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.accessibilitySupport": "off",
  "editor.colorDecorators": false,
  "editor.cursorStyle": "block",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.dragAndDrop": false,
  "editor.find.seedSearchStringFromSelection": "never",
  "editor.fontFamily": "'Inconsolata LGC'",
  "editor.fontSize": 17,
  "editor.formatOnSave": false,
  "editor.hideCursorInOverviewRuler": true,
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "editor.links": false,
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.selectionHighlight": false,
  "editor.overviewRulerBorder": false,
  "editor.parameterHints.enabled": false,
  "editor.quickSuggestions": {
    "other": false,
    "comments": false,
    "strings": false
  },
  "editor.renderLineHighlight": "none",
  "editor.rulers": [100, 120],
  "editor.suggestOnTriggerCharacters": false,
  "editor.guides.indentation": false,
  "explorer.enableDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "explorer.confirmDelete": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "html.format.endWithNewline": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "force-expand-multiline",
  "telemetry.telemetryLevel": "off",
  "update.mode": "none",
"window.title": "${dirty}${activeEditorShort}${separator}${rootName}",
  "window.restoreWindows": "one",
  "workbench.colorCustomizations": {
    "terminal.background": "#0B1C2C",
    "statusBar.background": "#005f5f",
    "statusBar.noFolderBackground": "#005f5f",
    "statusBar.debuggingBackground": "#005f5f"
},
  "workbench.editor.showTabs": false,
  "workbench.editor.limit.value": 4,
  "workbench.editor.limit.enabled": true,
  "workbench.settings.editor": "json",
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "none",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#005f5f",
  "vim.statusBarColors.insert": "#c0392b",
  "vim.statusBarColors.visual": "#913D88",
  "vim.statusBarColors.visualline": "#593281",
  "vim.statusBarColors.visualblock": "#86592d",
  "vim.statusBarColors.replace": "#000000",
  "vim.foldfix": true,
  "editor.codeActionsOnSave": {},
  "diffEditor.renderSideBySide": true,
  "cSpell.userWords": [],
  "workbench.editor.untitled.hint": "hidden",
  "extensions.autoUpdate": "onlyEnabledExtensions",
"editor.inlineSuggest.enabled": true,
"window.menuBarVisibility": "toggle",
"workbench.activityBar.visible": false,
"github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": false,
    "markdown": false
}
}
