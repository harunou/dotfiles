#!/bin/sh
# vim:set et sw=2:


if [ $# -gt 0 ]; then
  command="$1"
  shift
fi

case "$command" in
  setup)
    exec "harunou-setup" "$@"
    ;;

  config)
    exec "harunou-config" "$@"
    ;;

  '') echo "Usage: `basename "$0"` <command> [options]" 
    ;;

  *) 
    echo "`basename "$0"` $command: unknown command." >&2; exit 1 
    ;;
esac
